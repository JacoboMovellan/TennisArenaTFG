<?xml version="1.0" encoding="UTF-8"?>
        <?import javafx.geometry.Insets?>
        <?import javafx.scene.control.Label?>
        <?import javafx.scene.control.TextField?>
        <?import javafx.scene.control.PasswordField?>
        <?import javafx.scene.control.Button?>
        <?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.tennisarena.ControllerTenis">
<Label text="Registrarse" />
<TextField fx:id="registerEmailField" promptText="Correo Electrónico" />
<PasswordField fx:id="registerPasswordField" promptText="Contraseña" />
<PasswordField fx:id="confirmPasswordField" promptText="Confirmar Contraseña" />
<Button text="Registrarse" onAction="#onRegister" />
</VBox>
